{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/authentification/RegisterForm.jsx",
    "app/(tabs)/index.jsx",
    "app/(tabs)/upload.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/Mr. Tutor.jsx",
    "app/chatspot/chatspot.jsx",
    "app/_layout.jsx",
    "app/authentification/LoginForm.jsx",
    "app/authentification/ResetPasswordForm.jsx",
    "app/index.jsx",
    "app/profile/user.jsx",
    "nativewind-env.d.ts"
  ]
}